{
  "name": "05_npm",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node ./server.js",
    "dev": "nodemon ./server.js --env dev",
    "test": "nodemon ./server.js --env test",
    "simplex": "artillery quick --count 40 --num 50 'http://localhost:8080/simplex' -o ./test/quick/simplex.json",
    "complex": "artillery quick --count 40 --num 50 'http://localhost:8080/complex' -o ./test/quick/complex.json",
    "performance": "artillery run ./test/flow/flow.yaml --output ./test/flow/flow.json",
    "report": "artillery report ./test/flow/flow.html",
    "image": "docker build -t simplexcomplex .",
    "container": "docker run -p 8080:8080 simplexcomplex",
    "orquestra": "kubectl apply -f ./orquestra.yaml",
    "mocha": "mocha ./test/mocha",
    "chai": "mocha ./test/chai",
    "super": "mocha ./test/supertest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "-": "^0.0.1",
    "artillery-plugin-metrics-by-endpoint": "^1.5.0",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.3",
    "command": "^0.0.1",
    "commander": "^12.0.0",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-compression": "^1.0.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.3",
    "mongoose-paginate-v2": "^1.8.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-github": "^1.1.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.7.5",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^5.0.4",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/testing": "^10.3.8",
    "artillery": "^1.7.4",
    "chai": "^5.1.1",
    "mocha": "^10.4.0",
    "nodemon": "^3.1.0"
  },
  "directories": {
    "test": "test"
  },
  "description": ""
}
