  "scripts": {
    "start": "node ./server.js",
    "dev": "nodemon ./server.js --env dev",
    "test": "nodemon ./server.js --env test",
    "simplex": "artillery quick --count 40 --num 50 'http://localhost:8080/simplex' -o ./test/quick/simplex.json",
